
{
  "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "pretty": true,
      "sourceMap": true,
      "target": "es6",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "outDir": "./dist",
"baseUrl": ".",
      "paths":{
          "*": [
              "node_modules/*"
          ]

      }
  },
  "include": [
      "src/**/*"
  ]
}